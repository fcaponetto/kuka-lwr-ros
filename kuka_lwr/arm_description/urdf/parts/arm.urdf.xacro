<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">

<xacro:property name="mmTom" value="0.001" />
<xacro:property name="mm2Tom2" value="${0.001*0.001}" />
<xacro:property name="tonsToKg" value="1000.0" />

<xacro:property name="PI" value="3.14159265359" />
<xacro:property name="PI_2" value="1.57079632679" />

<xacro:property name="toRad" value="${PI/180.0}" />
<xacro:property name="toDeg" value="${180.0/PI}" />

<material name="grey">
  <color rgba="0.5 0.5 0.5 1"/>
</material>

<material name="red">
  <color rgba="1.0 0.0 0.0 1"/>
</material>

<!-- DYNAMICS PARAMETERS NEEDS TO BE CHEKED! -->

<link name ="_teleop_link0">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <mass value="1.5"/>
      <inertia ixx="${1e+03*mm2Tom2}"  ixy="0.0"  ixz="0.0" iyy="${1e+03*mm2Tom2}" iyz="0.0" izz="${1e+03*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link0_visual">
        <mesh filename="package://teleop_urdf/meshes/_teleop_link0.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link0_collision">
        <mesh filename="package://teleop_urdf/meshes/simple/_teleop_link0.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>

<link name ="_teleop_link1">
    <inertial>
      <origin xyz="0.0 ${-20.*mmTom} ${50.*mmTom}" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="${1e+03*mm2Tom2}"  ixy="0.0"  ixz="0.0" iyy="${1e+03*mm2Tom2}" iyz="0.0" izz="${800*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${PI_2} 0 ${-PI_2}"/>
      <geometry name="_teleop_link1_visual">
        <mesh filename="package://teleop_urdf/meshes/_teleop_link1.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${PI_2} 0 ${-PI_2}"/>
      <geometry name="_teleop_link1_collision">
        <mesh filename="package://teleop_urdf/meshes/simple/_teleop_link1.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>

<link name ="_teleop_link2">
    <inertial>
      <origin xyz="0.0 ${10*mmTom} ${180.*mmTom}" rpy="0 0 0"/>
      <mass value="3.3"/>
      <inertia ixx="${40e+03*mm2Tom2}"  ixy="0.0"  ixz="0.0" iyy="${40e+03*mm2Tom2}" iyz="0.0" izz="${10e3*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link2_visual">
        <mesh filename="package://teleop_urdf/meshes/_teleop_link2.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link2_collision">
        <mesh filename="package://teleop_urdf/meshes/simple/_teleop_link2.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>

<link name ="_teleop_link3">
    <inertial>
      <origin xyz="0.0 ${-20*mmTom} ${50*mmTom}" rpy="0 0 0"/>
      <mass value="0.3"/>
      <inertia ixx="${1e+03*mm2Tom2}"  ixy="0.0"  ixz="0.0" iyy="${1e+03*mm2Tom2}" iyz="0.0" izz="${800*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link3_visual">
        <mesh filename="package://teleop_urdf/meshes/_teleop_link3.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link3_collision">
        <mesh filename="package://teleop_urdf/meshes/simple/_teleop_link3.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>

<link name ="_teleop_link4">
    <inertial>
      <origin xyz="0.0 0.0 ${100*mmTom}" rpy="0 0 0"/>
      <mass value="2.5"/>
      <inertia ixx="${1.5e+04*mm2Tom2}"  ixy="0.0"  ixz="0.0" iyy="${1.5e+04*mm2Tom2}" iyz="0.0" izz="${8e3*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link4_visual">
        <mesh filename="package://teleop_urdf/meshes/_teleop_link4.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${PI_2} 0 0"/>
      <geometry name="_teleop_link4_collision">
        <mesh filename="package://teleop_urdf/meshes/simple/_teleop_link4.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>

<link name ="_teleop_link5">
    <inertial>
      <origin xyz="0.0 0.0 ${150*mmTom}" rpy="0 0 0"/>
      <mass value="2"/>
      <inertia ixx="${1.e+04*mm2Tom2}"  ixy="0.0"  ixz="0.0" iyy="${1.e+04*mm2Tom2}" iyz="0.0" izz="${1.e4*mm2Tom2}" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="${PI_2} 0 ${PI}"/>
      <geometry name="_teleop_link5_visual">
        <mesh filename="package://teleop_urdf/meshes/_teleop_link5.stl" scale="0.001 0.001 0.001" />
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="${PI_2} 0 ${PI}"/>
      <geometry name="_teleop_link5_collision">
        <mesh filename="package://teleop_urdf/meshes/simple/_teleop_link5.stl" scale="0.001 0.001 0.001" />
      </geometry>             
    </collision>
</link>

<joint name="joint1" type="revolute">
    <parent link="_teleop_link0"/>
    <child link="_teleop_link1"/>
    <origin xyz="0.0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="11.7" effort="55" lower="-1.75" upper="1.75" />
    <dynamics friction="0" damping="0.03"/>
</joint>

<joint name="joint2" type="revolute">
    <parent link="_teleop_link1"/>
    <child link="_teleop_link2"/>
    <origin xyz="0 0 ${131.5*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="5.2" effort="147" lower="-2.15" upper="2.15" />
    <dynamics friction="0" damping="0.03"/>
</joint>

<joint name="joint3" type="revolute">
    <parent link="_teleop_link2"/>
    <child link="_teleop_link3"/>
    <origin xyz="0 0 ${365*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="8.1" effort="127" lower="-2.15" upper="2.15" />
    <dynamics friction="0" damping="0.03"/>
</joint>

<joint name="joint4" type="revolute">
    <parent link="_teleop_link3"/>
    <child link="_teleop_link4"/>
    <origin xyz="0 0 ${100*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit velocity="20.4" effort="28" lower="-2.79" upper="2.79" />
    <dynamics friction="0" damping="0.03"/>
</joint>

<joint name="joint5" type="revolute">
    <parent link="_teleop_link4"/>
    <child link="_teleop_link5"/>
    <origin xyz="0 0 ${226*mmTom}" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="20.4" effort="28" lower="-1.83" upper="1.83" />
    <dynamics friction="0" damping="0.03"/>
</joint>

</robot>
